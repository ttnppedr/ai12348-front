        function getSortList(item) {
            return item.sub.map(function (v) {
                return '<div class="col-md-4"><a href="' + v.link + '">' + v.title + '</a></div>'
            })
        }

        function getSortChoose(e) {
            var i = $(this).data('index')
            var t = advisory[i]
            if (t.link) {
                location.href = t.link
                return
            }
            var list = getSortList(t)
            $('.sort-list--modal .modal-body .row').html(list.join(''))
            $('.sort-list--modal').modal('show')
        }

        $(function () {
            $('body')
                .on('click', '.main-advice a', getSortChoose)
                .on('click', '.gotop', function (e) {
                    $('html,body').animate({
                        scrollTop: 0
                    }, 'normal')
                })
            // '/get/statistics_12348/'
            $.get('/get/advice/total/?shop_id=102', function (res) {
                var options = {
                    useEasing: true,
                    useGrouping: true,
                    separator: ',',
                    decimal: '.',
                };
                var endCount = res.result.document + res.result.report
                var demo = new CountUp('staticCount', 0, endCount, 0, 2.5, options);
                if (!demo.error) {
                    demo.start();
                } else {
                    console.error(demo.error);
                }
            })
        });
    </script>
</body>

</html>